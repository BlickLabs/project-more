*
  margin 0
  padding 0

.section_one
  height 100vh
  background-image url('../img/backgrounds/section_1.jpg')
  background-size cover
  background-position center

.section_one-wrapper
  height 100vh

.section_one-container
  position relative
  max-width 1030px
  height 100vh
  center-block()
  display flex
  justify-content center
  flex-direction column

.section_one-title-wrapper
  display flex

.section_one-title-container
  max-width 522px

.section_one-title
  font-size remify(82)
  color #fff
  letter-spacing -1.7px
  line-height 1.14
  margin 0

.blue-text
  color $blueStandar

.section_one-description-wrapper
  display flex
  justify-content flex-end

.section_one-description-container
  max-width 496px

.section_one-description
  font-size remify(22)
  color #fff
  line-height 1.14
  margin 0

.section_one-blue-divider
  width 1px
  height 90px
  background-color $blueStandar
  center-block()
  position absolute
  bottom 0
  left 49.99%

.section_one-blue-box
  width 100%
  height 120px
  background-color $blueStandar
  bottom calc(-120px / 2)
  position absolute
  display flex
  flex-direction column
  justify-content flex-start
  align-items center

.section_one-white-divider-blue-box
  height calc(60% - 29px)
  width 1px
  background-color #fff

.section_one-cicle-arrow
  width 29px
  height 29px
  border-radius 50%
  background-color #fff
  total-center()
  -webkit-box-shadow 0px 0px 5px 11px rgba(0,0,0,0.08)
  -moz-box-shadow 0px 0px 5px 11px rgba(0,0,0,0.08)
  box-shadow 0px 0px 5px 11px rgba(0,0,0,0.08)
//----------------------fin section_one-----------------------------



